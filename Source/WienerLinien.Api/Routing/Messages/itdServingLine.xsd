<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:include schemaLocation="itdComplexTypes.xsd"/>
	<xsd:complexType name="itdServingLinesType">
		<xsd:annotation>
			<xsd:documentation>represents a list of serving lines</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdMessage" type="itdMessageType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="itdServingLine" type="itdServingLineType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="selected" type="xsd:string"/>
		<xsd:attribute name="trainInfo" default="hide">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="show"/>
					<xsd:enumeration value="hide"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="MOT0_selected" type="xsd:string"/>
		<xsd:attribute name="MOT0" default="hide">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="show"/>
					<xsd:enumeration value="hide"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="MOT1_selected" type="xsd:string"/>
		<xsd:attribute name="MOT1" default="hide">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="show"/>
					<xsd:enumeration value="hide"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="MOT2_selected" type="xsd:string"/>
		<xsd:attribute name="MOT2" default="hide">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="show"/>
					<xsd:enumeration value="hide"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="MOT3_selected" type="xsd:string"/>
		<xsd:attribute name="MOT3" default="hide">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="show"/>
					<xsd:enumeration value="hide"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="MOT4_selected" type="xsd:string"/>
		<xsd:attribute name="MOT4" default="hide">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="show"/>
					<xsd:enumeration value="hide"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="MOT5_selected" type="xsd:string"/>
		<xsd:attribute name="MOT5" default="hide">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="show"/>
					<xsd:enumeration value="hide"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="MOT6_selected" type="xsd:string"/>
		<xsd:attribute name="MOT6" default="hide">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="show"/>
					<xsd:enumeration value="hide"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="MOT7_selected" type="xsd:string"/>
		<xsd:attribute name="MOT7" default="hide">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="show"/>
					<xsd:enumeration value="hide"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="MOT8_selected" type="xsd:string"/>
		<xsd:attribute name="MOT8" default="hide">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="show"/>
					<xsd:enumeration value="hide"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="MOT9_selected" type="xsd:string"/>
		<xsd:attribute name="MOT9" default="hide">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="show"/>
					<xsd:enumeration value="hide"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="MOT10_selected" type="xsd:string"/>
		<xsd:attribute name="MOT10" default="hide">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="show"/>
					<xsd:enumeration value="hide"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="MOT11_selected" type="xsd:string"/>
		<xsd:attribute name="MOT11" default="hide">
