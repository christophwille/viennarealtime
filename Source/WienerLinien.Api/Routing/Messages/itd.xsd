<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="unqualified" attributeFormDefault="unqualified">
	<xsd:include schemaLocation="itdOptionTypes.xsd"/>
	<xsd:include schemaLocation="itdComplexTypes.xsd"/>
	<xsd:include schemaLocation="itdPersonalProfileType.xsd"/>
	<xsd:include schemaLocation="legTT.xsd"/>
	<xsd:include schemaLocation="itdAddInfo.xsd"/>
	<xsd:include schemaLocation="appInfo.xsd"/>
	<xsd:element name="itdRequest" type="itdRequestType"/>
	<xsd:complexType name="itdRequestType">
		<xsd:annotation>
			<xsd:documentation>document root element</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdMessageList" type="itdMessageListType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>container for request related messages</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="clientHeaderLines" type="clientHeaderLinesType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>supplies all client header lines from the request.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdVersionInfo" type="itdVersionInfoType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>supplies version information for application and data</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdPersonalProfile" type="itdPersonalProfileType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>contains personal profile if available</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdLayoutParams" type="itdLayoutParamsType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>this object is a container to transport variables from one request to the next whithout having a meaning to the server</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdInfoLinkList" type="itdInfoLinkListType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>this object is a container to transport real time messages independent from journey planner content</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="serverMetaInfo" type="serverMetaInfoType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>this object is a container in a distributed request concerning server information and their meta-data</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:element name="itdTripRequest" type="itdTripRequestType">
					<xsd:annotation>
						<xsd:documentation>trip request root element</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="itdDepartureMonitorRequest" type="itdDepartureMonitorRequestType">
					<xsd:annotation>
						<xsd:documentation>departure monitor request root element</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="itdAddInfoRequest" type="itdAddInfoRequestType">
					<xsd:annotation>
						<xsd:documentation>add info request root element</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="itdCoordInfoRequest" type="itdCoordInfoRequestType">
					<xsd:annotation>
						<xsd:documentation>coord info root element</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="itdParkObjectRequest" type="itdParkObjectRequestType">
					<xsd:annotation>
						<xsd:documentation>vehicle monitor root element</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:element name="appInfo" type="appInfo" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Comment describing your root element for updating application information</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="language" type="languageType" use="optional" default="en">
			<xsd:annotation>
				<xsd:documentation>represents the language of a certain request. (http://de.wikipedia.org/wiki/ISO_639)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="sessionID" type="sessionIDType" use="optional">
			<xsd:annotation>
				<xsd:documentation>represents the unique session id for each request</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="client" type="clientType" use="optional">
			<xsd:annotation>
				<xsd:documentation>represents the agent-name of the requesting client</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="serverID" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>describes the serverID defined in the server def file or the server host ip address if serverID is not specified</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="clientIP" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>specifies the client ip address if available</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="userName" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>If the request was authorized by a user/password validation, the userName will be added. Otherwise the attribute is missing.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="version" type="versionType" use="optional">
			<xsd:annotation>
				<xsd:documentation>includes the current server version</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="virtDir" type="virtDirType" use="optional">
			<xsd:annotation>
				<xsd:documentation>defines the name of the virtual directory called by the client request</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="now" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>TODO: JS Kommentar</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="nowWD" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>TODO: JS Kommentar</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="sendEmail" type="yesnoType" use="optional">
			<xsd:annotation>
				<xsd:documentation>determines whether the request should send mails or not</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="emailAddress" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>represents the recipient email address</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="lengthUnit" type="lengthUnitType">
			<xsd:annotation>
				<xsd:documentation>unit of all distances and altitudes used in current response</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="itdGeneralRequestType">
		<xsd:annotation>
			<xsd:documentation>general request</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="requestBaseType"/>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="itdTripRequestType">
		<xsd:annotation>
			<xsd:documentation>trip request root element</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="requestBaseType">
				<xsd:sequence>
					<xsd:element name="itdPrintConfiguration" type="itdPrintConfigurationType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>print configuration for professional requests</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="itdAddress" type="itdAddressType" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element name="itdDownload" type="itdDownloadType" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element name="itdOdv" type="itdOdvType">
						<xsd:annotation>
							<xsd:documentation>origin location</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="itdOdv" type="itdOdvType">
						<xsd:annotation>
							<xsd:documentation>destination location</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="itdOdv" type="itdOdvType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>one or more via locations</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="itdAddOdvSeq" type="itdAddOdvSeqType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>container for stop over e.g. sequence of journeys, P+R, etc.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="itdTripDateTime" type="itdTripDateTimeType"/>
					<xsd:element name="itdTripOptions" type="itdTripOptionsType"/>
					<xsd:element name="itdItinerary" type="itdItineraryType"/>
					<xsd:element name="itdBooking" type="itdBookingType" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="itdDepartureMonitorRequestType">
		<xsd:annotation>
			<xsd:documentation>departure monitor request root element</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="requestBaseType">
				<xsd:sequence>
					<xsd:element name="itdPrintConfiguration" type="itdPrintConfigurationType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>print configuration for professional requests</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="itdOdv" type="itdOdvType"/>
					<xsd:element name="itdDateTime" type="itdDateTimeType"/>
					<xsd:element name="itdDateRange" type="itdDateRangeType" minOccurs="0"/>
					<xsd:element name="itdOdv" type="itdOdvType" minOccurs="0"/>
					<xsd:element name="itdServingLines" type="itdServingLinesType" minOccurs="0"/>
					<xsd:element name="itdDepartureList" type="itdDepartureListType" minOccurs="0"/>
					<xsd:element name="itdArrivalList" type="itdArrivalListType" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="itdAddressType">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="name" type="xsd:string" use="required"/>
				<xsd:attribute name="place" type="xsd:string" use="required"/>
				<xsd:attribute name="street" type="xsd:string" use="required"/>
				<xsd:attribute name="addressExt1" type="xsd:string" use="required"/>
				<xsd:attribute name="addressExt2" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="itdBookingType">
		<xsd:sequence>
			<xsd:element name="itdBookingRouteList" type="itdBookingRouteListType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdBookingRouteListType">
		<xsd:sequence>
			<xsd:element name="itdBookingRoute" type="itdBookingRouteType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdBookingRouteType">
		<xsd:sequence>
			<xsd:element name="routeIdx" type="xsd:nonNegativeInteger"/>
			<xsd:element name="userBookingCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Bookingcode, which will presented to the customer.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="fromLocation" type="xsd:string"/>
			<xsd:element name="toLocation" type="xsd:string"/>
			<xsd:element name="userName" type="xsd:string" minOccurs="0"/>
			<xsd:element name="userPhone" type="xsd:string" minOccurs="0"/>
			<xsd:element name="passengerNum" type="xsd:nonNegativeInteger" default="1" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Complete number of passengers including children and wheelchairs</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="childNum" type="xsd:nonNegativeInteger" default="0" minOccurs="0"/>
			<xsd:element name="luggageNum" type="xsd:nonNegativeInteger" default="0" minOccurs="0"/>
			<xsd:element name="wheelchairNum" type="xsd:nonNegativeInteger" default="0" minOccurs="0"/>
			<xsd:element name="smoker" type="xsd:boolean" default="false" minOccurs="0"/>
			<xsd:element name="itdBookingPartialRouteList" type="itdBookingPartialRouteListType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdBookingPartialRouteListType">
		<xsd:sequence>
			<xsd:element name="itdBookingPartialRoute" type="itdBookingPartialRouteType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdBookingPartialRouteType">
		<xsd:sequence>
			<xsd:element name="partialRouteIdx" type="xsd:nonNegativeInteger"/>
			<xsd:element name="partialRouteBookingCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Bookingcode, which is used for internal use.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdBookingPartialRouteStatusList" type="itdBookingPartialRouteStatusListType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdBookingPartialRouteStatusListType">
		<xsd:sequence>
			<xsd:element name="itdBookingPartialRouteStatus" type="itdBookingPartialRouteStatusType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdBookingPartialRouteStatusType">
		<xsd:sequence>
			<xsd:element name="changeDate" type="xsd:dateTime"/>
			<xsd:element name="action" type="bookingAction"/>
			<xsd:element name="status" type="xsd:boolean"/>
			<xsd:element name="comment" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdDaysOfServiceType">
		<xsd:sequence>
			<xsd:element name="serviceMonth" type="serviceMonthType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="serviceRestrictionText" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="itdDaysOfServiceElem" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="ref" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="itdDepartureType">
		<xsd:sequence>
			<xsd:element name="itdDateTime" type="itdDateTimeType"/>
			<xsd:element name="itdRTDateTime" type="itdDateTimeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Definition-Parameter: XML DepartureMonitorRealDateTime 0|1</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdRefDaysOfService" type="itdRefDaysOfServiceType" minOccurs="0"/>
			<xsd:element name="itdFrequencyInfo" type="itdFrequencyInfoType" minOccurs="0"/>
			<xsd:element name="itdServingLine" type="itdServingLineType"/>
			<xsd:element name="itdResTL" type="itdResTLType" minOccurs="0"/>
			<xsd:element name="infoLinkRef" type="infoLinkRefType" minOccurs="0"/>
			<xsd:element name="itdInfoLinkList" type="itdInfoLinkList" minOccurs="0"/>
			<xsd:element name="genAttrList" type="genAttrListType" minOccurs="0"/>
			<xsd:element name="itdPrevStopSeq" type="itdStopSeqType" minOccurs="0"/>
			<xsd:element name="itdOnwardStopSeq" type="itdStopSeqType" minOccurs="0"/>
			<xsd:element name="itdPathCoordinates" type="itdCoordinateListType" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="stopID" type="xsd:string" default="0"/>
		<xsd:attribute name="x"/>
		<xsd:attribute name="y"/>
		<xsd:attribute name="mapName"/>
		<xsd:attribute name="stopName" type="xsd:string"/>
		<xsd:attribute name="area" type="xsd:string" default="0"/>
		<xsd:attribute name="platform" type="xsd:string"/>
		<xsd:attribute name="platformName" type="xsd:string"/>
		<xsd:attribute name="countdown" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="itdDepartureListType">
		<xsd:sequence>
			<xsd:element name="itdDeparture" type="itdDepartureType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="currentIndex" type="xsd:string" default="0"/>
	</xsd:complexType>
	<xsd:complexType name="itdArrivalListType">
		<xsd:sequence>
			<xsd:element name="itdArrival" type="itdDepartureType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="currentIndex" type="xsd:string" default="0"/>
	</xsd:complexType>
	<xsd:complexType name="itdFareOdvType">
		<xsd:sequence>
			<xsd:element name="itdOdvPlace" type="itdOdvPlaceType"/>
			<xsd:element name="itdFarePlace" type="itdFarePlaceType"/>
		</xsd:sequence>
		<xsd:attribute name="usage" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="origin"/>
					<xsd:enumeration value="destination"/>
					<xsd:enumeration value="via"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="itdFarePlaceType">
		<xsd:sequence>
			<xsd:element name="itdMessage" type="itdMessageType"/>
			<xsd:choice>
				<xsd:element name="odvNameElem" type="odvNameElemType" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:choice>
		</xsd:sequence>
		<xsd:attribute name="state" default="list">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="list"/>
					<xsd:enumeration value="identified"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="itdFootPathElemType">
		<xsd:annotation>
			<xsd:documentation>represents the footpath between two interchange stops</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdPoint" type="itdPointType"/>
			<xsd:element name="itdPoint" type="itdPointType"/>
			<xsd:choice>
				<xsd:element name="infoLink" type="infoLinkType" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:choice>
		</xsd:sequence>
		<xsd:attribute name="type" default="unknown">
			<xsd:annotation>
				<xsd:documentation>represents the type of foot path elem (e.g. stairs, escalator, ramp....)</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="LEVEL"/>
					<xsd:enumeration value="RAMP"/>
					<xsd:enumeration value="STAIRS"/>
					<xsd:enumeration value="ESCALATOR"/>
					<xsd:enumeration value="ELEVATOR"/>
					<xsd:enumeration value="ILLUMINATED"/>
					<xsd:enumeration value="DANGEROUS"/>
					<xsd:enumeration value="unknown"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="level" default="LEVEL">
			<xsd:annotation>
				<xsd:documentation>determines the level of the foot path element</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="LEVEL"/>
					<xsd:enumeration value="UP"/>
					<xsd:enumeration value="DOWN"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="description" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>textual description (not used)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="disrupted" type="yesnoType" default="0">
			<xsd:annotation>
				<xsd:documentation>additional information whether footpath element is blocked or out of order (real time information)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="itdFootPathInfoType">
		<xsd:annotation>
			<xsd:documentation>indicates whether the foot path need to be after or before the context partial route</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdFootPathElem" type="itdFootPathElemType" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="duration" type="xsd:string" default="0">
			<xsd:annotation>
				<xsd:documentation>indicates the duration of the foot path</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="position" default="AFTER">
			<xsd:annotation>
				<xsd:documentation>If attribute is 'IDEST' the context partial route is a foot path otherwise it is before or after the context partial route</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="BEFORE"/>
					<xsd:enumeration value="IDEST"/>
					<xsd:enumeration value="AFTER"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="itdFreqTripType">
		<xsd:sequence>
			<xsd:element name="itdDateTime" type="itdDateTimeType"/>
			<xsd:element name="itdDateTime" type="itdDateTimeType"/>
			<xsd:element name="itdMeansOfTransport" type="itdMeansOfTransportType"/>
			<xsd:element name="itdInfoTextList" type="itdInfoTextListType" minOccurs="0"/>
			<xsd:element name="itdPoint" type="itdPointType"/>
			<xsd:element name="itdPoint" type="itdPointType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdFreqTripListType">
		<xsd:sequence>
			<xsd:element name="itdFreqTrip" type="itdFreqTripType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdFrequencyInfoType">
		<xsd:annotation>
			<xsd:documentation>
				If frequency calculation is active this element will appear. (see EFAServer.def Section: Frequency)
				tripIndex attribute will indicate, if times for this leg have been shifted by the frequency calculation.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdFreqTripList" type="itdFreqTripListType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						This list will include the calculated frequency trips.
						The list starts with the index 1 and the attribtue tripIndex is a reference into this list.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdMeansOfTransportList" type="itdMeansOfTransportListType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						This list includes a set of unique lines serving the leg in the frequency calculation intervall.
						This list will be populated with information either if this leg is a frequncy leg or if this leg is a underground leg.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="depPlatform" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						After each means of transport a departure and / or arrival platform will b displayed.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="arrPlatform" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						After each means of transport a departure and / or arrival platform will b displayed.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="tripIndex" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>if tripIndex is &gt; 0 times from the appropriate itdFreqTripList have to be displayed for this leg</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="index" type="xsd:string"/>
		<xsd:attribute name="minTimeGap" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>
						If this leg is a frequency leg minTimeGap will describe the minumum time distance between possible alternative services.
					</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="maxTimeGap" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>
						If this leg is a frequency leg maxTimeGap will describe the maximum time distance between possible alternative services.
					</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="avTimeGap" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>
						If this leg is a frequency leg avTimeGap will describe the average time distance between possible alternative services.
					</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="minDuration" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>
						If this leg is a frequency leg minDuration will describe the minimum travel duration time inside the frequency time intervall.
					</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="maxDuration" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>
						If this leg is a frequency leg maxDuration will describe the maximum travel duration time inside the frequency time intervall.
					</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="avDuration" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>
						If this leg is a frequency leg avDuration will describe the average travel duration time inside the frequency time intervall.
					</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="timeFrom" type="xsd:string"/>
		<xsd:attribute name="timeTo" type="xsd:string"/>
		<xsd:attribute name="numberOfTrips" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="itdIMapParamsType">
		<xsd:attribute name="centerX" type="xsd:string"/>
		<xsd:attribute name="centerY" type="xsd:string"/>
		<xsd:attribute name="mapName" type="xsd:string"/>
		<xsd:attribute name="mapType" type="xsd:string"/>
		<xsd:attribute name="zoomLevel" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="itdImageType">
		<xsd:annotation>
			<xsd:documentation>represtents a map for e.g. interchange stop, environment or route option</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="src" type="xsd:string" use="required">
					<xsd:annotation>
						<xsd:documentation>represents the path to the map</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="size" type="xsd:string" default="0">
					<xsd:annotation>
						<xsd:documentation>represents the file size of the map</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="itdInfoTextListType">
		<xsd:annotation>
			<xsd:documentation>represents addtional text fed by the timetable (e.g. "lowfloor bus available", "hail and ride"...)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="pathDescriptionElem" type="pathDescriptionElemType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="infoTextListElem" type="infoTextListElemType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdItineraryType">
		<xsd:annotation>
			<xsd:documentation>if this node has children a trip calculation has been peformed otherwise it will remain empty</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdRouteList" type="itdRouteListType" minOccurs="0"/>
			<xsd:element name="infoLink" type="infoLinkType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="itdLegTTs" type="itdLegTTsType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdLayoutParamType">
		<xsd:annotation>
			<xsd:documentation>variable which should be transfered from one request to the next whithout having a meaning to the server (name=value)</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="name" type="xsd:string" use="required"/>
		<xsd:attribute name="value" type="xsd:string" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="itdLayoutParamsType">
		<xsd:annotation>
			<xsd:documentation>this type is a container to transport variables from one request to the next whithout having a meaning to the server</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdLayoutParam" type="itdLayoutParamType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdInfoLinkListType">
		<xsd:annotation>
			<xsd:documentation>represents additional information fed by ICS.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdIndexInfoList" type="itdAddInfoMessageListType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>represents a list of index information fed by ICS.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdBannerInfoList" type="itdAddInfoMessageListType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>represents a list of banner information fed by ICS.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdGeneralInfoList" type="itdAddInfoMessageListType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>represents a list of general information fed by ICS.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdStopBlockingList" type="itdAddInfoMessageListType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>represents a list of stop blockings fed by ICS.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdLineBlockingList" type="itdAddInfoMessageListType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>represents a list of line blockings fed by ICS.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdStopInfoList" type="itdAddInfoMessageListType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>represents a list of information about stops fed by ICS.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdLineInfoList" type="itdAddInfoMessageListType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>represents a list of information about lines fed by ICS.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdSpecificInfoList" type="itdAddInfoMessageListType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>represents a list of all available messages inside the request.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="serverMetaInfoType">
		<xsd:sequence>
			<xsd:element name="serviceAreaInfo" type="serviceAreaInfoType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="serviceAreaInfoType">
		<xsd:sequence>
			<xsd:element name="serviceArea" type="serviceAreaType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="serviceAreaType">
		<xsd:attribute name="name" type="xsd:string"/>
		<xsd:attribute name="net" type="xsd:string"/>
		<xsd:attribute name="selected" type="yesnoType"/>
	</xsd:complexType>
	<xsd:complexType name="itdLoginRequestType">
		<xsd:sequence>
			<xsd:element name="itdMessage" type="itdMessageType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="itdPersonalProfile" type="itdPersonalProfileType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="requestID" type="xsd:string" default="0"/>
		<xsd:attribute name="method" default="COOKIE">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="COOKIE"/>
					<xsd:enumeration value="BASIC"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="status" type="onoffType" default="off"/>
	</xsd:complexType>
	<xsd:complexType name="itdLogoffRequestType">
		<xsd:sequence>
			<xsd:element name="itdMessage" type="itdMessageType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="itdPersonalProfile" type="itdPersonalProfileType" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="requestID" type="xsd:string" default="0"/>
		<xsd:attribute name="method" default="COOKIE">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="COOKIE"/>
					<xsd:enumeration value="BASIC"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="status" type="onoffType" default="off"/>
	</xsd:complexType>
	<xsd:complexType name="itdNewPersonalProfileRequestType">
		<xsd:sequence>
			<xsd:element name="itdPersonalProfile" type="itdPersonalProfileType" minOccurs="0"/>
			<xsd:element name="itdMessage" type="itdMessageType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="requestID" type="xsd:string" default="0"/>
	</xsd:complexType>
	<xsd:complexType name="itdAsynchronousRequestType">
		<xsd:sequence>
			<xsd:element name="itdMessage" type="itdMessageType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="itdPrintConfiguration" type="itdPrintConfigurationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>print configuration for professional requests</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdAsynchronous" type="itdAsynchronousType"/>
		</xsd:sequence>
		<xsd:attribute name="requestID" type="xsd:string" default="0"/>
	</xsd:complexType>
	<xsd:complexType name="itdMapItemType">
		<xsd:annotation>
			<xsd:documentation>represents a map (PDF, PNG or JPG)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdImage" type="itdImageType"/>
		</xsd:sequence>
		<xsd:attribute name="text" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>textual description of the map (optional)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="type" default="RM">
			<xsd:annotation>
				<xsd:documentation>map type</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="RM">
						<xsd:annotation>
							<xsd:documentation>routing map (gis based)</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="CM">
						<xsd:annotation>
							<xsd:documentation>city map (raster map)</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="SM">
						<xsd:annotation>
							<xsd:documentation>static map</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="RE">
						<xsd:annotation>
							<xsd:documentation>DEPRECATED</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="IM">
						<xsd:annotation>
							<xsd:documentation>interactive map</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="ID">
						<xsd:annotation>
							<xsd:documentation>indoor map</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="RM_ID">
						<xsd:annotation>
							<xsd:documentation>routing map with indoor information</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="CM_ID">
						<xsd:annotation>
							<xsd:documentation>city map with indoor information</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="itdMapItemListType">
		<xsd:annotation>
			<xsd:documentation>list of maps</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdMapItem" type="itdMapItemType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdMapParamsType">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="resX" type="xsd:string"/>
				<xsd:attribute name="resY" type="xsd:string"/>
				<xsd:attribute name="coordX" type="xsd:string"/>
				<xsd:attribute name="coordY" type="xsd:string"/>
				<xsd:attribute name="imgWidth" type="xsd:string"/>
				<xsd:attribute name="imgHeight" type="xsd:string"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="itdMeansOfTransportType">
		<xsd:annotation>
			<xsd:documentation>means of transport description (service)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="motDivaParams" type="motDivaParamsType"/>
			<xsd:element name="itdRouteDescText" type="xsd:string" minOccurs="0"/>
			<xsd:element name="itdOperator" type="itdOperatorType" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="name" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>long name of the running service (e.g. &quot;S-Bahn S8&quot;)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="type" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>indicates the symbol of the service</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="shortname" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>short name of the running service (e.g. &quot;S8&quot;)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="destination" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>destination text of the running service</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="symbol" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>diva line short code</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="network" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>diva network of the running service (diva line)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="productName" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>product name of the running service (e.g. "S-Bahn")</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="trainType" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>train type (Deutsche Bahn services)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="trainName" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>train name (Deutsche Bahn; e.g. &quot;Richard Wagner&quot;)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="motType" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>indicates the mot (same as for means of transport in-/exclusion)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="TTB" type="yesnoType" use="optional" default="0">
			<xsd:annotation>
				<xsd:documentation>indicates whether the line can be used to produce timetables</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="STT" type="yesnoType" use="optional" default="0">
			<xsd:annotation>
				<xsd:documentation>indicates whether the line can be used to produce stop timetables</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="ROP" type="yesnoType" use="optional" default="0">
			<xsd:annotation>
				<xsd:documentation>indicates whether the line can be used to produce route option plans</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="destID" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>destination stop ID</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="addName" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>additional service name</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="spTr" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>additional special traffic information</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="itdMeansOfTransportListType">
		<xsd:annotation>
			<xsd:documentation>list of means of transports (for frequencies)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdMeansOfTransport" type="itdMeansOfTransportType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdPSDateType">
		<xsd:sequence>
			<xsd:element name="itdDate" type="itdDateType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="weekday" type="xsd:string" use="required"/>
		<xsd:attribute name="groupID" type="xsd:string" use="optional"/>
		<xsd:attribute name="validityTo" type="xsd:string" use="optional"/>
		<xsd:attribute name="validityFrom" type="xsd:string" use="optional"/>
	</xsd:complexType>
	<xsd:complexType name="itdPSDateTimeType">
		<xsd:sequence>
			<xsd:element name="itdValidityInfo" type="itdValidityInfoType"/>
			<xsd:element name="itdPSDate" type="itdPSDateType"/>
			<xsd:element name="itdPSTime" type="itdPSTimeType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdPSTimeType">
		<xsd:sequence>
			<xsd:element name="itdTimeFrom" type="itdTimeFromType"/>
			<xsd:element name="itdTimeTo" type="itdTimeToType"/>
		</xsd:sequence>
		<xsd:attribute name="deparr" default="int_arr">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="int_arr"/>
					<xsd:enumeration value="int_dep"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="maxIntervalHours" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="itdValidityInfoType">
		<xsd:sequence>
			<xsd:element name="itdValidityPeriod" type="itdValidityPeriodType" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="validTo" type="xsd:string"/>
		<xsd:attribute name="validFrom" type="xsd:string"/>
		<xsd:attribute name="messageCode" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="itdValidityPeriodType">
		<xsd:sequence>
			<xsd:element name="refDate" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="validTo" type="xsd:string"/>
		<xsd:attribute name="validFrom" type="xsd:string"/>
		<xsd:attribute name="description" type="xsd:string"/>
		<xsd:attribute name="weekdayType" type="xsd:string"/>
		<xsd:attribute name="groupID" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="itdPartialRouteType">
		<xsd:annotation>
			<xsd:documentation>represents a partial route of a trip</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdPoint" type="itdPointType"/>
			<xsd:element name="itdPoint" type="itdPointType"/>
			<xsd:element name="itdMeansOfTransport" type="itdMeansOfTransportType"/>
			<xsd:element name="itdRBLControlled" type="itdRBLControlledType" minOccurs="0"/>
			<xsd:element name="itdInfoTextList" type="itdInfoTextListType" minOccurs="0"/>
			<xsd:element name="itdPathDescription" type="itdPathDescriptionType" minOccurs="0"/>
			<xsd:element name="itdFootPathInfo" type="itdFootPathInfoType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="infoLink" type="infoLinkType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="itdFrequencyInfo" type="itdFrequencyInfoType" minOccurs="0"/>
			<xsd:element name="itdPrevStopSeq" type="itdStopSeqType" minOccurs="0"/>
			<xsd:element name="itdStopSeq" type="itdStopSeqType" minOccurs="0"/>
			<xsd:element name="itdOnwardStopSeq" type="itdStopSeqType" minOccurs="0"/>
			<xsd:element name="itdTaxiFare" type="itdTaxiFareType" minOccurs="0"/>
			<xsd:element name="itdViaList" type="itdViaListType" minOccurs="0"/>
			<xsd:element name="itdPathLinks" type="itdPathLinkType" minOccurs="0"/>
			<xsd:element name="itdPathCoordinates" type="itdCoordinateListType" minOccurs="0"/>
			<xsd:element name="itdITPathDescription" type="itdITPathDescriptionType" minOccurs="0"/>
			<xsd:element name="itdPathToll" type="itdPathTollType" minOccurs="0"/>
			<xsd:element name="itdInterchangePathCoordinates" type="itdInterchangePathCoordinatesType" minOccurs="0"/>
			<xsd:element name="genAttrList" type="genAttrListType" minOccurs="0"/>
			<xsd:element name="nextDeps" type="nextDepsType" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="active" type="yesnoType" default="1">
			<xsd:annotation>
				<xsd:documentation>indicates whether the route is active (0 = if waiting time between interchanges should be overnight)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="timeMinute" type="xsd:string" default="0">
			<xsd:annotation>
				<xsd:documentation>travel time of the partial route in minutes</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="type" default="PT">
			<xsd:annotation>
				<xsd:documentation>indicates whether partial route is traveling on PT or IT</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="PT"/>
					<xsd:enumeration value="IT"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="controlled" type="yesnoType" default="0">
			<xsd:annotation>
				<xsd:documentation>DEPRECATED</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="distance" type="xsd:string" default="0">
			<xsd:annotation>
				<xsd:documentation>distance between departure and arrival in meters</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="bookingCode" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>unique booking code for partial route</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="infoProvider" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>server id of passive server who calculated this partial route</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="hasFrequency" type="yesnoType" default="0">
			<xsd:annotation>
				<xsd:documentation>
					If this attribute is active the leg display should be a frequency display.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="partialRouteType" type="computePartialRouteType" default="normalPartialRoute"/>
	</xsd:complexType>
	<xsd:complexType name="itdViaListType">
		<xsd:annotation>
			<xsd:documentation>list of via types</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdVia" type="itdViaType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdViaType">
		<xsd:annotation>
			<xsd:documentation>represents the certain via-stop within a stop sequence between departure and arrival point if this via-stop is not an explicit interchange</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdDateTime" type="itdDateTimeType" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="stopID" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>represents the stop id of the via stop</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="itdPartialRouteListType">
		<xsd:annotation>
			<xsd:documentation>list of partial routes</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdPartialRoute" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="itdPartialRouteType">
							<xsd:attribute name="legTTID" type="xsd:string"/>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdPathDescriptionType">
		<xsd:annotation>
			<xsd:documentation>DEPRECATED</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="pathDescriptionElem" type="pathDescriptionElemType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdPeriodDateTimeType">
		<xsd:sequence>
			<xsd:element name="itdDate" type="itdDateType"/>
			<xsd:element name="itdDate" type="itdDateType"/>
			<xsd:element name="itdTime" type="itdTimeType"/>
			<xsd:element name="itdRefDaysOfService" type="itdRefDaysOfServiceType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdPersonalProfileRequestType">
		<xsd:sequence>
			<xsd:element name="ppOdvData" type="ppOdvDataType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="itdPersonalProfile" type="itdPersonalProfileType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="itdMessage" type="itdMessageType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="requestID" type="xsd:string" use="required"/>
		<xsd:attribute name="currentPage" type="xsd:string"/>
		<xsd:attribute name="command" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>added with version 9.11</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="itdPointType">
		<xsd:annotation>
			<xsd:documentation>represents a stop</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdMapItemList" type="itdMapItemListType" minOccurs="0"/>
			<xsd:element name="itdDateTime" type="itdDateTimeType" minOccurs="0" maxOccurs="2"/>
			<xsd:element name="itdDateTimeTarget" type="itdDateTimeType" minOccurs="0"/>
			<xsd:element name="genAttrList" type="genAttrListType" minOccurs="0"/>
			<xsd:element name="buildingInfo" type="buildingInfoType" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="stopID" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>represents the unique id of certain stop</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="area" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>represents the area of a certain stop (diva stop model)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="usage" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>represents the usage of the stop (e.g. departure, arrival...)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="name" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>represents the name of a certain stop</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="nameWO" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>represents the name without locality of a certain stop</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="place" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>represents the place location of a certain stop</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="platform" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>represents internal platform name (diva stop model)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="platformName" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>common platform name for output</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="hasStaticInfo" type="yesnoType" use="optional" default="0">
			<xsd:annotation>
				<xsd:documentation>indicates that extra information is available for this point</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="pbyb" type="yesnoType" use="optional" default="0">
			<xsd:annotation>
				<xsd:documentation>stop specific information</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="pathCoordinateIdx" type="xsd:unsignedInt" use="optional">
			<xsd:annotation>
				<xsd:documentation>index of the stop within the coordinate sequence</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="x" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>x coordinate value</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="y" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>y coordinate value</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="mapName" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>coordinate type</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="omc">
			<xsd:annotation>
				<xsd:documentation>municipality code</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="placeID">
			<xsd:annotation>
				<xsd:documentation>place identifier</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="locality">
			<xsd:annotation>
				<xsd:documentation>locality name</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="zone" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>stop fare zone</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="itdPrintConfigurationType">
		<xsd:sequence>
			<xsd:element name="printer" type="printerType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="active" type="xsd:string"/>
		<xsd:attribute name="layout" type="xsd:string"/>
		<xsd:attribute name="shading" type="yesnoType" default="1"/>
		<xsd:attribute name="serverInfo" type="xsd:string"/>
		<xsd:attribute name="serverAddress" type="xsd:string"/>
		<xsd:attribute name="outputFormat" type="xsd:string"/>
		<xsd:attribute name="printerDirect" type="yesnoType" default="0"/>
		<xsd:attribute name="fontSizeNormal" type="yesnoType" default="1"/>
		<xsd:attribute name="commuterFaresOutput" type="yesnoType" default="0"/>
		<xsd:attribute name="fareUnitOrientation" default="left">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="left"/>
					<xsd:enumeration value="right"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="itdPsRequestType">
		<xsd:complexContent>
			<xsd:extension base="requestBaseType">
				<xsd:sequence>
					<xsd:element name="itdPrintConfiguration" type="itdPrintConfigurationType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>print configuration for professional requests</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="itdAddress" type="itdAddressType" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element name="itdDownload" type="itdDownloadType" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element name="itdOdv" type="itdOdvType" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>itdOdv describes a location point object like stopping point, address, place of interest, ...</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="itdPSDateTime" type="itdPSDateTimeType"/>
					<xsd:element name="itdTripOptions" type="itdTripOptionsType"/>
					<xsd:element name="itdResTLInfo" type="itdResTLInfoType" minOccurs="0"/>
					<xsd:element name="itdItinerary" type="itdItineraryType"/>
				</xsd:sequence>
				<xsd:attribute name="onlyJourney" type="yesnoType" default="1"/>
				<xsd:attribute name="rideDirectionType" default="JOURNEY">
					<xsd:simpleType>
						<xsd:restriction base="xsd:NMTOKEN">
							<xsd:enumeration value="JOURNEY"/>
							<xsd:enumeration value="RETURN_TRIP"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="outputType" type="documentOutputType" default="HTML"/>
				<xsd:attribute name="psFileName" type="xsd:string"/>
				<xsd:attribute name="index" type="xsd:string" default="-1"/>
				<xsd:attribute name="layout" type="xsd:string"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="itdRouteRequestType">
		<xsd:complexContent>
			<xsd:extension base="requestBaseType">
				<xsd:sequence>
					<xsd:element name="itdPrintConfiguration" type="itdPrintConfigurationType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>print configuration for professional requests</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="itdOdv" type="itdOdvType" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>itdOdv describes a location point object like stopping point, address, place of interest, ...</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="itdPSDateTime" type="itdPSDateTimeType"/>
					<xsd:element name="itdTripOptions" type="itdTripOptionsType"/>
					<xsd:element name="itdItinerary" type="itdItineraryType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="itdRefDaysOfServiceType">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="ref" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="itdResTLType">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="RVB" type="xsd:string"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="itdRouteType">
		<xsd:annotation>
			<xsd:documentation>represents a route</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:sequence minOccurs="0">
				<xsd:element name="itdDateTime" type="itdDateTimeType"/>
				<xsd:element name="itdDateTime" type="itdDateTimeType"/>
			</xsd:sequence>
			<xsd:element name="itdMapItemList" type="itdMapItemListType"/>
			<xsd:element name="itdPartialRouteList" type="itdPartialRouteListType"/>
			<xsd:element name="itdFare" type="itdFareType"/>
			<xsd:element name="itdDaysOfService" type="itdDaysOfServiceType"/>
			<xsd:element name="itdToll" type="itdTollType" minOccurs="0"/>
			<xsd:element name="itdResTL" type="itdResTLType" minOccurs="0"/>
			<xsd:element name="genAttrList" type="genAttrListType" minOccurs="0"/>
			<xsd:element name="seqRoutes" type="seqRoutesType" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="active" type="yesnoType" use="optional" default="1">
			<xsd:annotation>
				<xsd:documentation>
					Set to 1 means that this route is active and should be displayed.
					Set to 0 means that this route is inactive and should not be displayed.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="delete" type="yesnoType" use="optional" default="0">
			<xsd:annotation>
				<xsd:documentation>set to 1 means that this route is already deleted</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="changes" type="xsd:integer" use="optional" default="5">
			<xsd:annotation>
				<xsd:documentation>number of interchanges for that route</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="distance" type="xsd:integer" use="optional" default="0">
			<xsd:annotation>
				<xsd:documentation>describe the distance in meter for that route</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="alternative" type="yesnoType" use="optional" default="0">
			<xsd:annotation>
				<xsd:documentation>if this route is a alternative calcualation result the flag is set to 1</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="duration" type="xsd:string" use="optional" default="0">
			<xsd:annotation>
				<xsd:documentation>duration of the entire route</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="publicDuration" type="xsd:string" use="optional" default="0">
			<xsd:annotation>
				<xsd:documentation>duration for public transport</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="individualDuration" type="xsd:string" use="optional" default="0">
			<xsd:annotation>
				<xsd:documentation>duration for individual transport</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="overallDuration" type="xsd:string" use="optional" default="0">
			<xsd:annotation>
				<xsd:documentation>DEPRECATED</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="print" type="yesnoType" use="optional" default="0">
			<xsd:annotation>
				<xsd:documentation>selected for printing?</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="routeIndex" type="xsd:string" use="optional" default="0">
			<xsd:annotation>
				<xsd:documentation>represents the calculation order</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="hasFrequency" type="yesnoType" use="optional" default="0"/>
		<xsd:attribute name="routeTripIndex" type="xsd:integer" use="optional" default="0"/>
		<xsd:attribute name="fDesc" type="xsd:string" use="optional" default="0">
			<xsd:annotation>
				<xsd:documentation>describes the route calculation function in more detail. e.g. TRIP_LATEST, TRIP_EARLIEST</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="cTime" type="xsd:string" use="required"/>
		<xsd:attribute name="searchMode" type="xsd:integer" use="optional" default="0"/>
		<xsd:attribute name="vehicleTime" type="xsd:integer" use="required">
			<xsd:annotation>
				<xsd:documentation>describes the time you are in the vehicle</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="distanceTimeStart" type="xsd:string" use="optional"/>
		<xsd:attribute name="distanceTimeDest" type="xsd:string" use="optional"/>
		<xsd:attribute name="searchTimeDest" type="xsd:string" use="optional"/>
		<xsd:attribute name="method" type="baseWorkerMethodType">
			<xsd:annotation>
				<xsd:documentation>see baseWorkerMethodType introduced with version 9.12</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="itdRouteListType">
		<xsd:annotation>
			<xsd:documentation>list of routes</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdRoute" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="itdRouteType">
							<xsd:attribute name="selected" type="yesnoType" use="optional" default="0">
								<xsd:annotation>
									<xsd:documentation>used for selection and deselection in the interface</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="itdStopFinderRequestType">
		<xsd:sequence>
			<xsd:element name="itdOdv" type="itdOdvType"/>
			<xsd:element name="itdDateTime" type="itdDateTimeType"/>
		</xsd:sequence>
		<xsd:attribute name="requestID" type="xsd:string" default="0"/>
	</xsd:complexType>
	<xsd:complexType name="itdStopListRequestType">
		<xsd:sequence>
			<xsd:element name="itdOdv" type="itdOdvListType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="requestID" type="xsd:string" default="0"/>
	</xsd:complexType>
	<xsd:complexType name="itdStopSeqType">
		<xsd:annotation>
			<xsd:documentation>stop sequence list (e.g. for stops which are served on a particular line)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdPoint" type="itdPointType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdTaxiFareType">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="currency" type="xsd:string" use="required"/>
				<xsd:attribute name="price" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="itdTimeFromType">
		<xsd:annotation>
			<xsd:documentation>start time for personal schedule calculation interval</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="hour" type="xsd:string" use="required"/>
		<xsd:attribute name="minute" type="xsd:string" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="itdTimeToType">
		<xsd:annotation>
			<xsd:documentation>end time for personal schedule calculation interval</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="hour" type="xsd:string" use="required"/>
		<xsd:attribute name="minute" type="xsd:string" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="itdTripDateTimeType">
		<xsd:annotation>
			<xsd:documentation>date, time and arrival/departure</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdDateTime" type="itdDateTimeType">
				<xsd:annotation>
					<xsd:documentation>defines a date time object</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdDateRange" type="itdDateRangeType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="deparr" default="dep">
			<xsd:annotation>
				<xsd:documentation>indicates whether the trip date/time is for departure or arrival</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="dep"/>
					<xsd:enumeration value="arr"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="ttpFrom" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>timetable periode start date</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="ttpTo" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>timetable periode end date</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="itdTripOptionsType">
		<xsd:sequence>
			<xsd:element name="itdPtOptions" type="itdPtOptionsType"/>
			<xsd:element name="itdItOptions" type="itdItOptionsType"/>
			<xsd:element name="itdMMOptions" type="itdMMOptionsType" minOccurs="0"/>
			<xsd:element name="itdCustomizedOptions" type="itdCustomizedOptionsType" minOccurs="0"/>
			<xsd:element name="itdUsedOptions" type="itdUsedOptionsType"/>
			<xsd:element name="itdOperators" type="itdOperatorsType" minOccurs="0"/>
			<xsd:element name="itdTransports" type="itdTransportsType" minOccurs="0"/>
			<xsd:element name="itdServices" type="itdServiceType" minOccurs="0"/>
			<xsd:element name="itdCarriers" type="itdCarriersType" minOccurs="0"/>
			<xsd:element name="outputOptions" type="outputOptionsType" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="userDefined" type="yesnoType">
			<xsd:annotation>
				<xsd:documentation>DEPRECATED</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="odvFarePlaceElem">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="value" type="xsd:string"/>
				<xsd:attribute name="selected" type="yesnoType" default="0"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="pathDescriptionElemType">
		<xsd:annotation>
			<xsd:documentation>DEPRECATED</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="absoluteTime" type="itdDateTimeType"/>
			<xsd:element name="edgeList" type="itdEdgeListType"/>
		</xsd:sequence>
		<xsd:attribute name="text" type="xsd:string"/>
		<xsd:attribute name="direction" type="xsd:string"/>
		<xsd:attribute name="length" type="xsd:string"/>
		<xsd:attribute name="turnText" type="xsd:string"/>
		<xsd:attribute name="relativeTime" type="xsd:string"/>
		<xsd:attribute name="turnAngle" default="0">
			<xsd:simpleType>
				<xsd:annotation>
					<xsd:documentation>361 means further straight ahaid</xsd:documentation>
				</xsd:annotation>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="0"/>
					<xsd:enumeration value="90"/>
					<xsd:enumeration value="180"/>
					<xsd:enumeration value="270"/>
					<xsd:enumeration value="360"/>
					<xsd:enumeration value="361"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="itdITPathDescriptionElemType">
		<xsd:annotation>
			<xsd:documentation>element describing a manoeuvre at an important point of the it route</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="turnDirection" type="turnDirectionType">
				<xsd:annotation>
					<xsd:documentation>abstract description of the turn direction</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="turningManoeuvre" type="turningManoeuvreType">
				<xsd:annotation>
					<xsd:documentation>abstract description of the turning manoeuvre</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdCoord" type="itdCoordType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>the coordinate that is associated with the path description element</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="streetname" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>streetname composed from streetName1 and streetName2 in the DIVA gis data seperated by '/'</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="directionHint" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>possible direction hint for the better understanding of the path description (i.e. 'follow signs in the direction München')</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="municipalityName" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>name of the municipality</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="fromPathLinkIdx" type="xsd:unsignedInt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>index of the from link in the itdLinkList</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="toPathLinkIdx" type="xsd:unsignedInt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>index of the to link in the itdLinkList</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tollSegementIdx" type="xsd:unsignedInt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>index of the toll segment</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="skyDirection" type="xsd:unsignedShort" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>sky direction [0-360] of the to link</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="traveltime" type="xsd:unsignedLong">
				<xsd:annotation>
					<xsd:documentation>travel time [sec] from the last to the actual path description</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="cumTraveltime" type="xsd:unsignedLong">
				<xsd:annotation>
					<xsd:documentation>cumulated travel time [sec] from the origin till the actual point of the path description</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="distance" type="xsd:unsignedLong">
				<xsd:annotation>
					<xsd:documentation>distance [m] from the last to the actual path description</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="cumDistance" type="xsd:unsignedLong">
				<xsd:annotation>
					<xsd:documentation>cumulated distance [m] from the origin till the actual point of the path description</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="infoLinkRef" type="infoLinkRefType" minOccurs="0"/>
			<xsd:element name="genAttrList" type="genAttrListType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>additional path description information</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="altitudeDiffUp" type="xsd:string" use="optional"/>
		<xsd:attribute name="altitudeDiffDown" type="xsd:string" use="optional"/>
	</xsd:complexType>
	<xsd:complexType name="itdTollSegmentType">
		<xsd:annotation>
			<xsd:documentation>toll for a passed segment</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="fromPathLinkIdx" type="xsd:unsignedInt" use="required">
			<xsd:annotation>
				<xsd:documentation>idx to a link where the current toll segment starts</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="toPathLinkIdx" type="xsd:unsignedInt">
			<xsd:annotation>
				<xsd:documentation>idx to a link where the current toll segment ends</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="tollInfoId" type="xsd:unsignedInt">
			<xsd:annotation>
				<xsd:documentation>reference id to more detailed external information</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="typeOfToll" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>Short description of the type of toll. Toll based on distance, passing a toll booth or a start-destination-relation. This entry should only be used for debugging.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="currency" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>Follwing currencies are supported: EUR, GBP, USD</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="toll" type="xsd:decimal" use="required">
			<xsd:annotation>
				<xsd:documentation>Amount of money in smallest currency unit (EUR -> Cent, GBP -> Pences)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="itdTollSegmentListType">
		<xsd:annotation>
			<xsd:documentation>list of all toll segments passed </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence minOccurs="0" maxOccurs="unbounded">
			<xsd:element name="itdTollSegment" type="itdTollSegmentType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdPathTollType">
		<xsd:annotation>
			<xsd:documentation>element holding toll information of a partial route</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdTollSegments" type="itdTollSegmentListType"/>
		</xsd:sequence>
		<xsd:attribute name="currency" type="xsd:string" use="required"/>
		<xsd:attribute name="toll" type="xsd:decimal" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="itdTollVehicle">
		<xsd:annotation>
			<xsd:documentation>toll for vehicle type</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="vehicleType" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>type of the vehicle</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="currency" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>currency, which is used for the toll</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="toll" type="xsd:decimal" use="required">
			<xsd:annotation>
				<xsd:documentation>toll for a vehicle class</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="itdTollVehicleListType">
		<xsd:annotation>
			<xsd:documentation>list of tolls per vehicle type</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence maxOccurs="unbounded">
			<xsd:element name="itdTollVehicle" type="itdTollVehicle">
				<xsd:annotation>
					<xsd:documentation>For every vehicle class a seperate entry should be added to the sequence. If there are several partial routes with the same vehicle class, the toll is summed up for that vehicle class and is added as one entry.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdTollType">
		<xsd:annotation>
			<xsd:documentation>element holding the general toll information of a route</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdTollVehicleList" type="itdTollVehicleListType">
				<xsd:annotation>
					<xsd:documentation>tolls for the different vehicle classes in the route</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="currency" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>currency, which is used for the toll</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="overallToll" type="xsd:decimal" use="required">
			<xsd:annotation>
				<xsd:documentation>toll over all vehicle classes</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="serviceMonthType">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="month" type="xsd:string" use="required"/>
				<xsd:attribute name="year" type="xsd:string" use="required"/>
				<xsd:attribute name="dosCode" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="itdMapRequestType">
		<xsd:complexContent>
			<xsd:extension base="requestBaseType">
				<xsd:sequence>
					<xsd:element name="itdOdv" type="itdOdvType"/>
					<xsd:element name="mapPinList" type="itdMapPinListType"/>
					<xsd:element name="itdGisDrawClasses" type="itdGisDrawClassListType"/>
					<xsd:element name="itdMap" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>map stream for direct mode</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="action" type="xsd:string"/>
				<xsd:attribute name="fileName" type="xsd:string"/>
				<xsd:attribute name="zoomLevel" type="xsd:string"/>
				<xsd:attribute name="xCenterReal" type="xsd:string"/>
				<xsd:attribute name="yCenterReal" type="xsd:string"/>
				<xsd:attribute name="mapName" type="xsd:string"/>
				<xsd:attribute name="realWidth" type="xsd:string"/>
				<xsd:attribute name="realHeight" type="xsd:string"/>
				<xsd:attribute name="screenSize" type="xsd:string"/>
				<xsd:attribute name="screenScaleWidth" type="xsd:string"/>
				<xsd:attribute name="realScaleWidth" type="xsd:string"/>
				<xsd:attribute name="screenWidth" type="xsd:string"/>
				<xsd:attribute name="screenHeight" type="xsd:string"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="itdMapPinListType">
		<xsd:sequence>
			<xsd:element name="itdPinElem" type="itdMapPinType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdMapPinType">
		<xsd:sequence>
			<xsd:element name="itdCoord" type="itdMapCoordType"/>
			<xsd:element name="bitmapPinArea" type="itdBitmapPinAreaType" minOccurs="0"/>
			<xsd:element name="genAttrList" type="genAttrListType" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="type" type="mapPinType"/>
		<xsd:attribute name="desc" type="xsd:string"/>
		<xsd:attribute name="id" type="xsd:string"/>
		<xsd:attribute name="omc" type="xsd:string"/>
		<xsd:attribute name="hasStaticInfo" type="xsd:string"/>
		<xsd:attribute name="distance" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="itdAsynchronousType">
		<xsd:sequence>
			<xsd:element name="itdDownload" type="itdDownloadType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="asynchronousName" type="xsd:string"/>
		<xsd:attribute name="asynchronousOutputType" type="asynchronousOutputType"/>
	</xsd:complexType>
	<xsd:complexType name="itdAPsRequestType">
		<xsd:sequence>
			<xsd:element name="itdAddress" type="itdAddressType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="itdAsynchronous" type="itdAsynchronousAnswerType"/>
			<xsd:element name="itdOdv" type="itdOdvType" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>itdOdv describes a location point object like stopping point, address, place of interest, ...</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdPSDateTime" type="itdPSDateTimeType"/>
			<xsd:element name="itdTripOptions" type="itdTripOptionsType"/>
			<xsd:element name="itdResTLInfo" type="itdResTLInfoType" minOccurs="0"/>
			<xsd:element name="itdItinerary" type="itdItineraryType"/>
		</xsd:sequence>
		<xsd:attribute name="requestID" type="xsd:string" default="0"/>
		<xsd:attribute name="onlyJourney" type="yesnoType" default="1"/>
		<xsd:attribute name="rideDirectionType" default="JOURNEY">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="JOURNEY"/>
					<xsd:enumeration value="RETURN_TRIP"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="outputType" type="documentOutputType" default="HTML"/>
		<xsd:attribute name="psFileName" type="xsd:string"/>
		<xsd:attribute name="index" type="xsd:string" default="-1"/>
		<xsd:attribute name="layout" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="itdAsynchronousAnswerType">
		<xsd:sequence>
			<xsd:element name="itdMessage" type="itdMessageType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="asynchronousName" type="xsd:string"/>
		<xsd:attribute name="asynchronousHour" type="xsd:string"/>
		<xsd:attribute name="asynchronousMinute" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="itdPDADepartureMonitorRequestType">
		<xsd:annotation>
			<xsd:documentation>departure monitor request root element</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="requestBaseType">
				<xsd:sequence>
					<xsd:element name="itdOdv" type="itdOdvType"/>
					<xsd:element name="itdServingLines" type="itdServingLinesType" minOccurs="0"/>
					<xsd:element name="itdResTLInfo" type="itdResTLInfoType" minOccurs="0"/>
					<xsd:element name="itdDepartureList" type="itdDepartureListType" minOccurs="0"/>
					<xsd:element name="itdDownload" type="itdDownloadType" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="itdResTLInfoType">
		<xsd:sequence>
			<xsd:element name="itdResTLValidFrom" type="itdResTLValidFromType"/>
			<xsd:element name="itdResTLValidUntil" type="itdResTLValidUntilType"/>
			<xsd:element name="itdResTLStart" type="itdResTLStartType"/>
			<xsd:element name="itdResTLMonth" type="itdResTLMonthType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdResTLValidFromType">
		<xsd:sequence>
			<xsd:element name="itdDate" type="itdDateType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdResTLValidUntilType">
		<xsd:sequence>
			<xsd:element name="itdDate" type="itdDateType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdResTLStartType">
		<xsd:sequence>
			<xsd:element name="itdDate" type="itdDateType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdResTLMonthType">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="count" type="xsd:string"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="itdAddInfoRequestType">
		<xsd:sequence>
			<xsd:element name="timeStamp" type="addInfoDateTimeType" minOccurs="0"/>
			<xsd:element name="itdAdditionalTravelInformations" type="itdAdditionalTravelInformationsType" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="requestID" type="xsd:string" use="optional"/>
	</xsd:complexType>
	<xsd:complexType name="itdLocalityType">
		<xsd:sequence>
			<xsd:element name="itdLocalityElem" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="itdLocalityElemType">
							<xsd:attribute name="ccID" type="xsd:string" use="optional"/>
							<xsd:attribute name="tlID" type="xsd:string" use="optional"/>
							<xsd:attribute name="x" type="xsd:string" use="optional"/>
							<xsd:attribute name="y" type="xsd:string" use="optional"/>
							<xsd:attribute name="mapName" type="xsd:string" use="optional"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="state" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="itdLocalityElemType">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="name" type="xsd:string"/>
				<xsd:attribute name="nptgID" type="xsd:string"/>
				<xsd:attribute name="value" type="xsd:string"/>
				<xsd:attribute name="listIndex" type="xsd:string"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	
	
	<xsd:complexType name="itdLineStopType">
		<xsd:attribute name="omc" type="xsd:string"/>
		<xsd:attribute name="index" type="xsd:int"/>
		<xsd:attribute name="selected" type="yesnoType"/>
		<xsd:attribute name="placeID" type="xsd:string"/>
		<xsd:attribute name="stopID" type="xsd:string"/>
		<xsd:attribute name="divaID" type="xsd:string"/>
		<xsd:attribute name="area" type="xsd:string"/>
		<xsd:attribute name="platform" type="xsd:string"/>
		<xsd:attribute name="name" type="xsd:string"/>
		<xsd:attribute name="nameWithPlace" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="itdOperatorLinesType">
		<xsd:annotation>
			<xsd:documentation>describes all lines dependant to the operator</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdServingLine" type="itdServingLineType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdOperatorAppendantLinesType">
		<xsd:annotation>
			<xsd:documentation>describes the operator and the appendant lines</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdOperatorLines" type="itdOperatorLinesType"/>
		</xsd:sequence>
		<xsd:attribute name="net" type="xsd:string"/>
		<xsd:attribute name="code" type="xsd:string"/>
		<xsd:attribute name="name" type="xsd:string"/>
		<xsd:attribute name="selected" type="yesnoType" use="optional" default="0"/>
		<xsd:attribute name="type" type="inclType" use="optional" default="UNKNOWN"/>
	</xsd:complexType>
	<xsd:complexType name="itdOperatorsType">
		<xsd:sequence>
			<xsd:element name="itdOperator" type="itdOperatorAppendantLinesType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdOperatorRequestType">
		<xsd:complexContent>
			<xsd:extension base="requestBaseType">
				<xsd:sequence>
					<xsd:element name="itdOperators" type="itdOperatorsType" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="itdTransportsType">
		<xsd:annotation>
			<xsd:documentation>describes all means of transport modes for must include, must exclude, prefer to include and prefer to exclude</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdTransport" type="itdTransportType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdTransportType">
		<xsd:annotation>
			<xsd:documentation>describes all means of transports for must include, must exclude, prefer to include and prefer to exclude</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="transport" type="xsd:string"/>
		<xsd:attribute name="id" type="xsd:nonNegativeInteger"/>
		<xsd:attribute name="type" type="inclType"/>
	</xsd:complexType>
	<xsd:complexType name="itdServiceType">
		<xsd:annotation>
			<xsd:documentation>describes all lines for must include, must exclude, prefer to include and prefer to exclude</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdServingLine" type="itdServingLineType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdMessageListType">
		<xsd:annotation>
			<xsd:documentation>list of messages</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence minOccurs="0" maxOccurs="unbounded">
			<xsd:element name="itdMessage" type="itdMessageType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdITPathDescriptionType">
		<xsd:annotation>
			<xsd:documentation>element holding the complete path description of an it route </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdITPathDescriptionList" type="itdITPathDescriptionListType"/>
		</xsd:sequence>
		<xsd:attribute name="altitudeDiffUp" type="xsd:string" use="optional"/>
		<xsd:attribute name="altitudeDiffDown" type="xsd:string" use="optional"/>
	</xsd:complexType>
	<xsd:complexType name="itdITPathDescriptionListType">
		<xsd:annotation>
			<xsd:documentation>element holding the list of it path descriptions</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdITPathDescriptionElem" type="itdITPathDescriptionElemType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdLinkType">
		<xsd:annotation>
			<xsd:documentation>link involved in an it route</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="linkId" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>link id based on the DIVA gis network</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="linkDirection" type="linkDirectionType">
				<xsd:annotation>
					<xsd:documentation>direction of the link (forward = from point -&gt; to point; backward = to point -&gt; from point)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="flags" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>internal mdv flags</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="drawClass" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>internal mdv draw class based on the functional roadclasses or the funtionality of the link (i.e. railtrack, cycle path)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itDataSource" type="itDataSourceType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>data source include in the cost computation of a link inside the routing algorithm (static data, profile data, incident data, online data) </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="cumCost" type="xsd:long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>cumulated monetary costs from the origin till the actual end point of the link</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="cumDistance" type="xsd:long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>cumulated distance [m] from the origin till the actual end point of the link</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="cumTraveltime" type="xsd:long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>cumulated traveltime [sec] from the origin till the actual end point of the link</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdLinkSeqType">
		<xsd:annotation>
			<xsd:documentation>element holding the list of links of an it route</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdLink" type="itdLinkType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdPathLinkType">
		<xsd:annotation>
			<xsd:documentation>element holding a gis edge list of an it route</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="networkName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>name of the gis network where the link ids are valid</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="originPointOffset" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>distance of origin link segment not passed by route</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="destinationPointOffset" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>distance of destination link segment not passed by route</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="itdLinkList" type="itdLinkSeqType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdCoordinateBaseFormatType">
		<xsd:annotation>
			<xsd:documentation>describes the reference system of a coordinate</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="coordEllipsoid" type="coordinateEllipsoidType" nillable="true">
				<xsd:annotation>
					<xsd:documentation>reference ellipsoid on which the coordinates depend, i.e. WGS84</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="coordType" type="coordinateFormatType" nillable="true">
				<xsd:annotation>
					<xsd:documentation>type of the coordinate format (geographical or projection)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdCoordinateBaseType">
		<xsd:annotation>
			<xsd:documentation>coordinate base type used in different enhanced coordinate definitions</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="x" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>x value or longitude of a coordinate depending on coordType</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="y" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>y value or latitude of a coordinate depending on coordType</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="z" type="xsd:decimal" nillable="true" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>z value or height above ellipsoid depending on coordType</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdCoordinateType">
		<xsd:annotation>
			<xsd:documentation>element for a single coordinate</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="itdCoordinateBaseFormatType">
				<xsd:sequence>
					<xsd:element name="x" type="xsd:decimal">
						<xsd:annotation>
							<xsd:documentation>x value or longitude of a coordinate depending on coordType </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="y" type="xsd:decimal">
						<xsd:annotation>
							<xsd:documentation>y value or latitude of a coordinate depending on coordType</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="z" type="xsd:decimal" nillable="true" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>z value or height above ellipsoid depending on coordType</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="itdCoordinateBaseListType">
		<xsd:annotation>
			<xsd:documentation>base type for a list of coordinates</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdCoordinateBaseElem" type="itdCoordinateBaseType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdCoordinateListType">
		<xsd:annotation>
			<xsd:documentation>element holding a list of coordinates</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="itdCoordinateBaseFormatType">
				<xsd:sequence>
					<xsd:choice>
						<xsd:element name="itdCoordinateBaseElemList" type="itdCoordinateBaseListType">
							<xsd:annotation>
								<xsd:documentation>coordinate list representation as a sequence of coordinate elements</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="itdCoordinateString" type="itdCoordinateStringType">
							<xsd:annotation>
								<xsd:documentation>string representation of a coordinate list based on GML standard (http://opengis.net/gml/) </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:choice>
					<xsd:element name="itdCoordinateReductionList" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="itdCoordinateReduction" type="itdCoordinateReductionType" minOccurs="0" maxOccurs="unbounded"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="itdCoordinateReductionType">
		<xsd:annotation>
			<xsd:documentation>element holding a list of indices of reduced path coordinates regarding epsilon</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="epsilon" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>epsilon = maximum error between 2 points</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="itdInterchangePathCoordinatesType">
		<xsd:annotation>
			<xsd:documentation>element holding a list of interchange path coordinates</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdPathCoordinates" type="itdCoordinateListType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdCoordinateStringType">
		<xsd:annotation>
			<xsd:documentation>string representation of a coordinate list based on GML standard (http://opengis.net/gml/) </xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="decimal" type="xsd:string" default=".">
					<xsd:annotation>
						<xsd:documentation>separator for decimals</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="cs" type="xsd:string" default=",">
					<xsd:annotation>
						<xsd:documentation>separator for coordinate values</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="ts" type="xsd:string" default="&#x20;">
					<xsd:annotation>
						<xsd:documentation>attribute gives the tuple separator (a single space by default)</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="itdEdgeType">
		<xsd:attribute name="id" type="xsd:string"/>
		<xsd:attribute name="flags" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="itdEdgeListType">
		<xsd:sequence maxOccurs="unbounded">
			<xsd:element name="edge" type="itdEdgeType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdOdvRequestType">
		<xsd:sequence>
			<xsd:element name="itdOdv" type="itdOdvType"/>
			<xsd:element name="itdServingLines" type="itdServingLinesType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdConnStopListType">
		<xsd:sequence>
			<xsd:element name="stopGroup" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="name"/>
						<xsd:element name="groupElemList">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="groupElem" maxOccurs="unbounded">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="itdLineStop" type="itdLineStopType"/>
												<xsd:element name="itdServingLines" type="itdServingLinesType"/>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdSystemInfoRequestType">
		<xsd:sequence>
			<xsd:element name="itdVersionInfo" type="itdVersionInfoType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdVehicleMonitorRequestType">
		<xsd:annotation>
			<xsd:documentation>vehicle monitor root element</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="requestBaseType">
				<xsd:sequence>
					<xsd:element name="itdOdv" type="itdOdvType">
						<xsd:annotation>
							<xsd:documentation>departure location</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="itdOdv" type="itdOdvType">
						<xsd:annotation>
							<xsd:documentation>additional change location</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="itdDateTime" type="itdDateTimeType"/>
					<!-- <xsd:element name="itdTripOptions" type="itdTripOptionsType"/> -->
					<xsd:element name="itdVehicleInfoSeq" type="itdVehicleInfoSeqType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="itdVehicleInfoSeqType">
		<xsd:annotation>
			<xsd:documentation>container of all vehicle information</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdVehicleInfo" type="itdVehicleInfoType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdVehicleInfoType">
		<xsd:annotation>
			<xsd:documentation>container of all vehicle information</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdVehicleKey" type="itdVehicleInfoKeyType"/>
			<xsd:element name="lastStop" type="itdVehiclePointType"/>
			<xsd:element name="nextStop" type="itdVehiclePointType"/>
			<xsd:element name="depStop" type="itdVehiclePointType"/>
			<xsd:element name="arrStop" type="itdVehiclePointType"/>
			<xsd:element name="itdCoord" type="itdCoordType"/>
			<xsd:element name="itdPathCoordinates" type="itdCoordinateListType" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="posPrognosis" type="yesnoType"/>
	</xsd:complexType>
	<xsd:complexType name="itdVehicleInfoKeyType">
		<xsd:annotation>
			<xsd:documentation>DIVA vehicle information</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="motDivaParams" type="motDivaParamsType"/>
		</xsd:sequence>
		<xsd:attribute name="tripCode" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="itdVehiclePointType">
		<xsd:annotation>
			<xsd:documentation>Vehicle point information for last, next, departure and arrival stop</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdPoint" type="itdPointType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="coordInfoItemType">
		<xsd:sequence>
			<xsd:element name="itdPathCoordinates" type="itdCoordinateListType"/>
			<xsd:element name="genAttrList" type="genAttrListType" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="type"/>
		<xsd:attribute name="id"/>
		<xsd:attribute name="name"/>
		<xsd:attribute name="omc"/>
		<xsd:attribute name="placeID"/>
		<xsd:attribute name="locality"/>
		<xsd:attribute name="gisLayer"/>
		<xsd:attribute name="gisID"/>
		<xsd:attribute name="distance"/>
	</xsd:complexType>
	<xsd:complexType name="coordInfoFilterItemType">
		<xsd:attribute name="type"/>
		<xsd:attribute name="radius"/>
		<xsd:attribute name="inclDrawClasses"/>
		<xsd:attribute name="exclLayers"/>
		<xsd:attribute name="centerCoord"/>
		<xsd:attribute name="ratingMethod"/>
	</xsd:complexType>
	<xsd:complexType name="mapRectType">
		<xsd:sequence>
			<xsd:element name="itdCoord" type="itdCoordType"/>
		</xsd:sequence>
		<xsd:attribute name="halfWidth" type="xsd:string" use="required"/>
		<xsd:attribute name="halfHeight" type="xsd:string" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="lineItemListType">
		<xsd:sequence>
			<xsd:element name="lineItem" type="lineItemType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="lineItemType">
		<xsd:sequence>
			<xsd:element name="itdServingLine" type="itdServingLineType"/>
			<xsd:element name="itdPathCoordinates" type="itdCoordinateListType" maxOccurs="unbounded"/>
			<xsd:element name="itdPoint" type="itdPointType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="mapRectLineItem" type="mapRectType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="buildingInfoType">
		<xsd:sequence>
			<xsd:element name="buildingInfoItem" type="buildingInfoItem" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="buildingInfoItem">
		<xsd:sequence>
			<xsd:element name="itdMapItem" type="itdMapItemType" minOccurs="0"/>
			<xsd:element name="table" type="tableType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="tableType">
		<xsd:sequence>
			<xsd:element name="title" type="xsd:string"/>
			<xsd:element name="description" type="xsd:string"/>
			<xsd:element name="columns" type="xsd:string"/>
			<xsd:element name="row" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="separator" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="nextDepsType">
		<xsd:annotation>
			<xsd:documentation>next departures per leg</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdDateTime" type="itdDateTimeType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdAddOdvSeqType">
		<xsd:sequence>
			<xsd:element name="itdMessage" type="itdMessageType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="itdAddOdv" type="itdAddOdvType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itdParkObjectRequestType">
		<xsd:annotation>
			<xsd:documentation>trip request root element</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="requestBaseType">
				<xsd:sequence>
					<xsd:element name="itdOdv" type="itdOdvType">
						<xsd:annotation>
							<xsd:documentation>origin location</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="itdOdv" type="itdOdvType">
						<xsd:annotation>
							<xsd:documentation>destination location</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="itdTripDateTime" type="itdTripDateTimeType"/>
					<xsd:element name="itParking" type="itdParkingOptionType"/>
					<xsd:element name="itdAddOdvSeq" type="itdAddOdvSeqType">
						<xsd:annotation>
							<xsd:documentation>container for stop over e.g. sequence of journeys, P+R, etc.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="itdCoordInfoRequestType">
		<xsd:annotation>
			<xsd:documentation>geo object request root element</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="requestBaseType">
				<xsd:sequence>
					<xsd:element name="itdCoordInfo">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="coordInfoRequest" minOccurs="0">
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="itdCoord" minOccurs="0">
												<xsd:complexType>
													<xsd:attribute name="x"/>
													<xsd:attribute name="y"/>
													<xsd:attribute name="mapName"/>
												</xsd:complexType>
											</xsd:element>
											<xsd:element name="coordInfoFilterItemList" minOccurs="0">
												<xsd:complexType>
													<xsd:sequence>
														<xsd:element name="coordInfoFilterItem" type="coordInfoFilterItemType" maxOccurs="unbounded"/>
													</xsd:sequence>
												</xsd:complexType>
											</xsd:element>
										</xsd:sequence>
										<xsd:attribute name="max"/>
										<xsd:attribute name="purpose"/>
									</xsd:complexType>
								</xsd:element>
								<xsd:element name="coordInfoItemList" minOccurs="0">
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="coordInfoItem" type="coordInfoItemType" maxOccurs="unbounded"/>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="itdGeoObjectRequestType">
		<xsd:annotation>
			<xsd:documentation>geo object request root element</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="requestBaseType">
				<xsd:sequence>
					<xsd:element name="itdGeoObject">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="geoObjectLineRequest" minOccurs="0">
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="itdServingLines" type="itdServingLinesType" maxOccurs="unbounded"/>
											<xsd:element name="mapRect" type="mapRectType"/>
										</xsd:sequence>
										<xsd:attribute name="filterDate" type="xsd:string"/>
										<xsd:attribute name="filterStopID" type="xsd:string"/>
										<xsd:attribute name="filterEdgeID" type="xsd:string"/>
										<xsd:attribute name="returnSinglePath" type="xsd:string" use="required"/>
										<xsd:attribute name="reductionEpsilon" type="xsd:string" use="required"/>
									</xsd:complexType>
								</xsd:element>
								<xsd:element name="geoObjectLineResponse" minOccurs="0">
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="lineItemList" type="lineItemListType"/>
											<xsd:element name="mapRectLineItemList" type="mapRectType"/>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="itdIsoChroneRequestType">
		<xsd:annotation>
			<xsd:documentation>iso chrone request root element</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="requestBaseType">
				<xsd:sequence>
					<xsd:element name="isoChroneRequest">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="itdCoord" type="itdCoordType"/>
							</xsd:sequence>
							<xsd:attribute name="mot" type="xsd:integer" use="required"/>
							<xsd:attribute name="isoChroneSeconds" type="xsd:integer" use="required"/>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="isoChroneItemList">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="itdCoord" type="itdCoordType" minOccurs="0"/>
								<xsd:element name="isoChroneItem" minOccurs="0" maxOccurs="unbounded">
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="isoChroneCoords" type="itdCoordinateListType" minOccurs="0"/>
											<xsd:element name="isoPointsCoords" type="itdCoordinateListType" minOccurs="0"/>
										</xsd:sequence>
										<xsd:attribute name="isoChroneSeconds" type="xsd:integer" use="required"/>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
							<xsd:attribute name="mot" type="xsd:integer" use="required"/>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="itdPrintOutRequestType">
		<xsd:annotation>
			<xsd:documentation>output request root element</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="requestBaseType">
				<xsd:sequence>
					<xsd:element name="itdPrintOut" type="itdPrintOutType" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="itdPrintOutType">
		<xsd:annotation>
			<xsd:documentation>print output type element</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdDownload" type="itdDownloadType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="printOut" type="printOutType" default="UNKNOWN"/>
	</xsd:complexType>
	<xsd:complexType name="itdStartDestConnectionType">
		<xsd:annotation>
			<xsd:documentation>one connection between two points</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdDateTimeStart" type="itdDateTimeType"/>
			<xsd:element name="itdDateTimeDest" type="itdDateTimeType"/>
		</xsd:sequence>
		<xsd:attribute name="startId" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>ID of start point for connection. Only stop IDs allowed</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="destId" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>ID of destination point for connection. Only stop IDs allowed</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="connection" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>Values: 0 = co connection, 1 = connection not found, 2 = connection found</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="itdStartDestConnectionSeqType">
		<xsd:annotation>
			<xsd:documentation>connection container for m*n matrix connection</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="itdStartDestConnection" type="itdStartDestConnectionType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="direction" type="linkDirectionType" default="FORWARD"/>
	</xsd:complexType>
	<xsd:complexType name="seqRoutesType">
		<xsd:annotation>
			<xsd:documentation>describes connected journeys as partial routes inside one journey</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="seqRoute" minOccurs="0" maxOccurs="unbounded"/>
