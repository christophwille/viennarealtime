<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:include schemaLocation="itdComplexTypes.xsd"/>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="itdAdditionalTravelInformationsType">
		<xsd:sequence>
			<xsd:element name="status" type="xsd:string" minOccurs="0"/>
			<xsd:element name="itdAdditionalTravelInformation" type="itdAdditionalTravelInformationType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="itdAdditionalTravelInformationType">
		<xsd:sequence>
			<xsd:element name="sourceSystem" type="sourceSystemType" minOccurs="0"/>
			<xsd:element name="reasonGroup" type="reasonGroupType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Structured Classification Elements. Corresponds to TPEG 18 Event Reason.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="effectCondition" type="effectConditionEnumeration" minOccurs="0"/>
			<xsd:element name="creationTime" type="addInfoDateTimeType" minOccurs="0"/>
			<xsd:element name="lastModificationTime" type="addInfoDateTimeType" minOccurs="0"/>
			<xsd:element name="rejectionDate" type="addInfoDateTimeType" minOccurs="0"/>
			<xsd:element name="efaServiceSendingDateTime" type="addInfoDateTimeType" minOccurs="0"/>
			<xsd:element name="publicationDuration" type="aiDateTimeIntervalType"/>
			<xsd:element name="validityPeriod" type="aiDateTimeIntervalType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="expirationDateTime" type="addInfoDateTimeType" minOccurs="0"/>
			<xsd:element name="references" type="referencesType" minOccurs="0"/>
			<xsd:element name="addInfoItems" type="addInfoItemsType" minOccurs="0"/>
			<xsd:element name="infoLink" type="infoLinkType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="itIncident" type="itIncidentType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Abbildung der betroffenen Elemente, deren textuelle Beschreibung und der  resultieren Auswirkungen einer Verkehrssituation</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="concernedLines" type="concernedLinesType" minOccurs="0"/>
			<xsd:element name="concernedStops" type="concernedStopsType" minOccurs="0"/>
			<xsd:element name="genericParams" type="genericParamsType" minOccurs="0"/>
			<xsd:element name="XTISParams" type="genericParamsType" minOccurs="0"/>
			<xsd:element name="TPEGParams" type="genericParamsType" minOccurs="0"/>
			<xsd:element name="image" type="imageType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="file" type="xsd:string" use="optional"/>
		<xsd:attribute name="type" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="bannerInfo"/>
					<xsd:enumeration value="indexInfo"/>
					<xsd:enumeration value="generalInfo"/>
					<xsd:enumeration value="stopInfo"/>
					<xsd:enumeration value="stopBlocking"/>
					<xsd:enumeration value="lineInfo"/>
					<xsd:enumeration value="lineBlocking"/>
					<xsd:enumeration value="routeInfo"/>
					<xsd:enumeration value="routeBlocking"/>
					<xsd:enumeration value="interchangeDisruption"/>
					<xsd:enumeration value="trafficSituation"/>
					<xsd:enumeration value="carParkBlocking"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="infoID" type="xsd:string" use="required"/>
		<xsd:attribute name="seqID" type="xsd:decimal" use="required"/>
		<xsd:attribute name="deactivated" type="xsd:string" use="required"/>
		<xsd:attribute name="affectTimetable" type="xsd:string" use="optional"/>
		<xsd:attribute name="affectAIRequest" type="xsd:string" use="optional"/>
		<xsd:attribute name="authorName" type="xsd:string" use="optional"/>
		<xsd:attribute name="authorFirstName" type="xsd:string" use="optional"/>
		<xsd:attribute name="authorSymbol" type="xsd:string" use="optional"/>
		<xsd:attribute name="providerCode" type="xsd:string" use="optional"/>
		<xsd:attribute name="publish" type="xsd:string" use="optional"/>
		<xsd:attribute name="valid" type="xsd:string" use="optional"/>
		<xsd:attribute name="priority" use="optional">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="unknown"/>
					<xsd:enumeration value="veryLow"/>
					<xsd:enumeration value="low"/>
					<xsd:enumeration value="normal"/>
					<xsd:enumeration value="high"/>
					<xsd:enumeration value="veryHigh"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="blockingType" type="xsd:string" use="optional"/>
		<xsd:attribute name="eventStatus" type="xsd:string" use="optional"/>
		<xsd:attribute name="externID" type="xsd:string" use="optional"/>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="sourceSystemType">
		<xsd:sequence>
			<xsd:element name="systemID" type="xsd:string"/>
			<xsd:element name="systemName" type="xsd:string"/>
			<xsd:element name="systemType" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:simpleType name="effectConditionEnumeration">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="altered"/>
			<xsd:enumeration value="cancelled"/>
			<xsd:enumeration value="delayed"/>
			<xsd:enumeration value="diverted"/>
			<xsd:enumeration value="noService"/>
			<xsd:enumeration value="disrupted"/>
			<xsd:enumeration value="additionalService"/>
			<xsd:enumeration value="specialService"/>
			<xsd:enumeration value="onTime"/>
			<xsd:enumeration value="normalService"/>
			<xsd:enumeration value="intermittentService"/>
			<xsd:enumeration value="shortFormedService"/>
			<xsd:enumeration value="fullLengthService"/>
			<xsd:enumeration value="extendedService"/>
			<xsd:enumeration value="splittingTrain"/>
			<xsd:enumeration value="replacementTransport"/>
			<xsd:enumeration value="arrivesEarly"/>
			<xsd:enumeration value="shuttleService"/>
			<xsd:enumeration value="replacementService"/>
			<xsd:enumeration value="undefinedServiceInformation"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="reasonGroupType">
		<xsd:choice>
			<xsd:element ref="unknownReason"/>
			<xsd:element ref="miscellaneousReason"/>
			<xsd:element ref="personnelReason"/>
			<xsd:element ref="equipmentReason"/>
			<xsd:element ref="environmentReason"/>
			<xsd:element name="undefinedReason" type="xsd:string"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:element name="unknownReason" type="xsd:string"/>
	<!-- ###################################################################################################### -->
	<xsd:element name="miscellaneousReason" type="miscellaneousReasonEnumeration"/>
	<xsd:simpleType name="miscellaneousReasonEnumeration">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="incident"/>
			<xsd:enumeration value="nearMiss"/>
			<xsd:enumeration value="safetyViolation"/>
			<xsd:enumeration value="signalPassedAtDanger"/>
			<xsd:enumeration value="stationOverrun"/>
			<xsd:enumeration value="trainDoor"/>
			<xsd:enumeration value="bombExplosion"/>
			<xsd:enumeration value="securityAlert"/>
			<xsd:enumeration value="policeRequest"/>
			<xsd:enumeration value="fireBrigadeSafetyChecks"/>
			<xsd:enumeration value="unattendedBag"/>
			<xsd:enumeration value="telephonedThreat"/>
			<xsd:enumeration value="suspectVehicle"/>
			<xsd:enumeration value="fire"/>
			<xsd:enumeration value="linesideFire"/>
			<xsd:enumeration value="vandalism"/>
			<xsd:enumeration value="accident"/>
			<xsd:enumeration value="fatality"/>
			<xsd:enumeration value="personUnderTrain"/>
			<xsd:enumeration value="personHitByTrain"/>
			<xsd:enumeration value="emergencyServices"/>
			<xsd:enumeration value="personIllOnVehicle"/>
			<xsd:enumeration value="collision"/>
			<xsd:enumeration value="overcrowded"/>
			<xsd:enumeration value="insufficientDemand"/>
			<xsd:enumeration value="lightingFailure"/>
			<xsd:enumeration value="leaderBoardFailure"/>
			<xsd:enumeration value="serviceIndicatorFailure"/>
			<xsd:enumeration value="serviceFailure"/>
			<xsd:enumeration value="operatorCeasedTrading"/>
			<xsd:enumeration value="operatorSuspended"/>
			<xsd:enumeration value="congestion"/>
			<xsd:enumeration value="routeBlockage"/>
			<xsd:enumeration value="personOnTheLine"/>
			<xsd:enumeration value="vehicleOnTheLine"/>
			<xsd:enumeration value="levelCrossingIncident"/>
			<xsd:enumeration value="objectOnTheLine"/>
			<xsd:enumeration value="fallenTreeOnTheLine"/>
			<xsd:enumeration value="vegetation"/>
			<xsd:enumeration value="trainStruckAnimal"/>
			<xsd:enumeration value="trainStruckObject"/>
			<xsd:enumeration value="animalOnTheLine"/>
			<xsd:enumeration value="routeDiversion"/>
			<xsd:enumeration value="roadClosed"/>
			<xsd:enumeration value="roadworks"/>
			<xsd:enumeration value="specialEvent"/>
			<xsd:enumeration value="bridgeStrike"/>
			<xsd:enumeration value="overheadObstruction"/>
			<xsd:enumeration value="undefinedProblem"/>
			<xsd:enumeration value="passenger"/>
			<xsd:enumeration value="passengerAction"/>
			<xsd:enumeration value="railwayCrime"/>
			<xsd:enumeration value="staffAssault"/>
			<xsd:enumeration value="emergencyServicesCall"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ###################################################################################################### -->
	<xsd:element name="personnelReason" type="personnelReasonEnumeration"/>
	<xsd:simpleType name="personnelReasonEnumeration">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="staffSickness"/>
			<xsd:enumeration value="staffInjury"/>
			<xsd:enumeration value="contractorStaffInjury"/>
			<xsd:enumeration value="staffAbsence"/>
			<xsd:enumeration value="staffInWrongPlace"/>
			<xsd:enumeration value="staffShortage"/>
			<xsd:enumeration value="industrialAction"/>
			<xsd:enumeration value="unofficialIndustrialAction"/>
			<xsd:enumeration value="workToRule"/>
			<xsd:enumeration value="undefinedPersonnelProblem"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ###################################################################################################### -->
	<xsd:element name="equipmentReason" type="equipmentReasonEnumeration"/>
	<xsd:simpleType name="equipmentReasonEnumeration">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="pointsProblem"/>
			<xsd:enumeration value="pointsFailure"/>
			<xsd:enumeration value="signalProblem"/>
			<xsd:enumeration value="trainWarningSystemProblem"/>
			<xsd:enumeration value="trackCircuitProblem"/>
			<xsd:enumeration value="signalFailure"/>
			<xsd:enumeration value="derailment"/>
			<xsd:enumeration value="engineFailure"/>
			<xsd:enumeration value="tractionFailure"/>
			<xsd:enumeration value="breakDown"/>
			<xsd:enumeration value="technicalProblem"/>
			<xsd:enumeration value="brokenRail"/>
			<xsd:enumeration value="poorRailConditions"/>
			<xsd:enumeration value="wheelImpactLoad"/>
			<xsd:enumeration value="repairWork"/>
			<xsd:enumeration value="constructionWork"/>
			<xsd:enumeration value="maintenanceWork"/>
			<xsd:enumeration value="emergencyEngineeringWork"/>
			<xsd:enumeration value="lateFinishToEngineeringWork"/>
			<xsd:enumeration value="lackOfOperationalStock"/>
			<xsd:enumeration value="powerProblem"/>
			<xsd:enumeration value="fuelProblem"/>
			<xsd:enumeration value="swingBridgeFailure"/>
			<xsd:enumeration value="escalatorFailure"/>
			<xsd:enumeration value="liftFailure"/>
			<xsd:enumeration value="gangwayProblem"/>
			<xsd:enumeration value="closedForMaintenance"/>
			<xsd:enumeration value="fuelShortage"/>
			<xsd:enumeration value="deicingWork"/>
			<xsd:enumeration value="wheelProblem"/>
			<xsd:enumeration value="luggageCarouselProblem"/>
			<xsd:enumeration value="undefinedEquipmentProblem"/>
			<xsd:enumeration value="defectiveFireAlarmEquipment"/>
			<xsd:enumeration value="defectiveCctv"/>
			<xsd:enumeration value="defectivePlatformEdgeDoors"/>
			<xsd:enumeration value="defectivePublicAnnouncementSystem"/>
			<xsd:enumeration value="ticketingSystemNotAvailable"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ###################################################################################################### -->
	<xsd:element name="environmentReason" type="environmentReasonEnumeration"/>
	<xsd:simpleType name="environmentReasonEnumeration">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="fog"/>
			<xsd:enumeration value="roughSea"/>
			<xsd:enumeration value="heavySnowFall"/>
			<xsd:enumeration value="driftingSnow"/>
			<xsd:enumeration value="blizzardConditions"/>
			<xsd:enumeration value="heavyRain"/>
			<xsd:enumeration value="strongWinds"/>
			<xsd:enumeration value="stormConditions"/>
			<xsd:enumeration value="stormDamage"/>
			<xsd:enumeration value="tidalRestrictions"/>
			<xsd:enumeration value="highTide"/>
			<xsd:enumeration value="lowTide"/>
			<xsd:enumeration value="ice"/>
			<xsd:enumeration value="frozen"/>
			<xsd:enumeration value="hail"/>
			<xsd:enumeration value="sleet"/>
			<xsd:enumeration value="highTemperatures"/>
			<xsd:enumeration value="flooding"/>
			<xsd:enumeration value="waterlogged"/>
			<xsd:enumeration value="lowWaterLevel"/>
			<xsd:enumeration value="highWaterLevel"/>
			<xsd:enumeration value="fallenLeaves"/>
			<xsd:enumeration value="fallenTree"/>
			<xsd:enumeration value="landslide"/>
			<xsd:enumeration value="undefinedEnvironmentalProblem"/>
			<xsd:enumeration value="lightningStrike"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ###################################################################################################### -->
	<xsd:element name="undefinedReason" type="xsd:string"/>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="addInfoDateTimeType">
		<xsd:sequence>
			<xsd:element name="itdDateTime" type="aiDateTimeType"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="aiDateTimeIntervalType">
		<xsd:sequence>
			<xsd:element name="itdDateTime" type="aiDateTimeType"/>
			<xsd:element name="itdDateTime" type="aiDateTimeType"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="aiDateTimeType">
		<xsd:sequence>
			<xsd:element name="itdDate" type="aiDateType"/>
			<xsd:element name="itdTime" type="aiTimeType"/>
		</xsd:sequence>
		<xsd:attribute name="absolute" type="xsd:string" use="optional"/>
		<xsd:attribute name="utc" type="xsd:string" use="optional"/>
		<xsd:attribute name="ttpFrom" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>timetable periode start date</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="ttpTo" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>timetable periode end date</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="aiDateType">
		<xsd:attribute name="day" type="xsd:decimal" use="required"/>
		<xsd:attribute name="month" type="xsd:decimal" use="required"/>
		<xsd:attribute name="year" type="xsd:decimal" use="required"/>
		<xsd:attribute name="weekday" use="optional" default="-1">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="1"/>
					<xsd:enumeration value="2"/>
					<xsd:enumeration value="3"/>
					<xsd:enumeration value="4"/>
					<xsd:enumeration value="5"/>
					<xsd:enumeration value="6"/>
					<xsd:enumeration value="7"/>
					<xsd:enumeration value="-1"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="aiTimeType">
		<xsd:attribute name="hour" type="xsd:decimal" use="required"/>
		<xsd:attribute name="minute" type="xsd:decimal" use="required"/>
		<xsd:attribute name="second" type="xsd:decimal" use="optional"/>
		<xsd:attribute name="ampm" type="xsd:string"/>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="referencesType">
		<xsd:sequence>
			<xsd:element name="parentReference" type="referenceType" minOccurs="0"/>
			<xsd:element name="childReference" type="referenceType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="referenceType">
		<xsd:sequence>
			<xsd:element name="type" type="referenceTypeEnumeration"/>
			<xsd:element name="messageID" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:simpleType name="referenceTypeEnumeration">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="cause"/>
			<xsd:enumeration value="effect"/>
			<xsd:enumeration value="association"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="infoLinkType">
		<xsd:choice>
			<xsd:sequence>
				<xsd:element name="paramList" type="aiParamListType" minOccurs="0"/>
				<xsd:element name="infoLinkText" type="xsd:string"/>
				<xsd:element name="infoLinkURL" type="xsd:string"/>
				<xsd:element name="infoLinkImage" type="xsd:string" minOccurs="0"/>
				<xsd:element name="infoText" type="infoTextType"/>
			</xsd:sequence>
			<xsd:sequence>
				<xsd:element name="shortText" type="xsd:string"/>
				<xsd:element name="longText" type="xsd:string"/>
				<xsd:element name="expectedDelays" type="xsd:string" minOccurs="0"/>
			</xsd:sequence>
		</xsd:choice>
		<xsd:attribute name="language" type="aiLanguageType" use="optional" default="en"/>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="addInfoItemsType">
		<xsd:sequence>
			<xsd:element name="addInfoItem" type="addInfoItemType" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="infoID" type="xsd:string" use="required"/>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="addInfoItemType">
		<xsd:sequence>
			<xsd:choice>
				<xsd:sequence>
					<xsd:element name="text" type="xsd:string"/>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element name="url" type="xsd:string"/>
					<xsd:element name="urlText" type="xsd:string" minOccurs="0"/>
					<xsd:element name="target" type="xsd:string" minOccurs="0"/>
				</xsd:sequence>
			</xsd:choice>
			<xsd:element name="targetMedia" type="targetMediaTypeEnum" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="type" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="title"/>
					<xsd:enumeration value="subtitle"/>
					<xsd:enumeration value="content"/>
					<xsd:enumeration value="additionalText"/>
					<xsd:enumeration value="url"/>
					<xsd:enumeration value="internalNotes"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="language" type="xsd:language"/>
		<xsd:attribute name="layout" type="xsd:string"/>
		<xsd:attribute name="encoding">
			<xsd:simpleType>
				<xsd:restriction base="xsd:NMTOKEN">
					<xsd:enumeration value="base64"/>
					<xsd:enumeration value="ansi"/>
					<xsd:enumeration value="html"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:simpleType name="targetMediaTypeEnum">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="all"/>
			<xsd:enumeration value="journeyPlanner"/>
			<xsd:enumeration value="timeTable"/>
			<xsd:enumeration value="sms"/>
			<xsd:enumeration value="wap"/>
			<xsd:enumeration value="ticker"/>
			<xsd:enumeration value="stationDisplay"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="genericParamsType">
		<xsd:sequence>
			<xsd:element name="paramList" type="aiParamListType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="XTISParamsType">
		<xsd:sequence>
			<xsd:element name="paramList" type="aiParamListType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="TPEGParamsType">
		<xsd:sequence>
			<xsd:element name="paramList" type="aiParamListType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="roadEventParamsType">
		<xsd:sequence>
			<xsd:element name="paramList" type="aiParamListType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="aiParamListType">
		<xsd:sequence>
			<xsd:element name="param" type="aiparamType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="aiparamType">
		<xsd:sequence>
			<xsd:element name="type" type="xsd:string"/>
			<xsd:element name="name" type="xsd:string"/>
			<xsd:element name="value" type="xsd:string"/>
		</xsd:sequence>
		<xsd:attribute name="edit" type="xsd:string" use="optional"/>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="infoTextType">
		<xsd:sequence>
			<xsd:element name="content" type="xsd:string"/>
			<xsd:element name="subtitle" type="xsd:string"/>
			<xsd:element name="subject" type="xsd:string"/>
			<xsd:element name="additionalText" type="xsd:string" minOccurs="0"/>
			<xsd:element name="internalNotes" type="xsd:string" minOccurs="0"/>
			<xsd:element name="image" type="xsd:string" minOccurs="0"/>
			<xsd:element name="outputClientText" type="outputClientTextType"/>
			<xsd:element name="additionalLinks" type="additionalLinksType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="outputClientTextType">
		<xsd:sequence>
			<xsd:element name="htmlText" type="xsd:string" minOccurs="0"/>
			<xsd:element name="wmlText" type="xsd:string" minOccurs="0"/>
			<xsd:element name="smsText" type="xsd:string" minOccurs="0"/>
			<xsd:element name="speechText" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="additionalLinksType">
		<xsd:sequence>
			<xsd:element name="additionalLink" type="additionalLinkType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:complexType name="additionalLinkType">
		<xsd:sequence>
			<xsd:element name="linkURL" type="xsd:string"/>
			<xsd:element name="linkText" type="xsd:string"/>
			<xsd:element name="linkTextShort" type="xsd:string" minOccurs="0"/>
			<xsd:element name="linkTarget" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="ID" type="xsd:string" use="optional"/>
	</xsd:complexType>
	<!-- ###################################################################################################### -->
	<xsd:simpleType name="aiLanguageType">
